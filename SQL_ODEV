QUESTION 1
SELECT  first_name,last_name FROM actor
WHERE first_name IN ('Nick','Ed','Jennifer')

QUESTION 2
SELECT  last_name FROM actor
WHERE first_name IN ('Nick','Ed','Jennifer')

QUESTION 3
SELECT * FROM public.address

QUESTION 4
SELECT district, phone  FROM public.address
ORDER BY district DESC, phone DESC;

QUESTION 5
SELECT f.film_id, f.title, i.inventory_id  FROM film f
JOIN inventory i ON f.film_id = i.film_id;

QUESTION 6
SELECT * FROM rental
JOIN inventory ON rental.inventory_id= inventory.inventory_id
LIMIT 5

QUESTION 7
SELECT r.rental_id, r.rental_date, p.payment_id, p.amount FROM rental r
JOIN payment p  ON (r.rental_id=p.rental_id)
ORDER BY p.amount DESC
LIMIT 10

QUESTION 8
SELECT * FROM actor
WHERE actor_id IS NULL

QUESTION 9
SELECT * FROM actor
WHERE actor_id IS NOT NULL

QUESTION 10
SELECT COUNT(*)  
FROM actor  
WHERE actor IS NOT NULL;

QUESTION 11
SELECT SUM(amount) AS sum_amt  
FROM payment;

QUESTION 12
SELECT MAX (amount), MIN (amount) FROM payment


